<div class="col-lg-4 col-md-4 col-sm-4">
  <div class="latest_post">
    <h2><span>All postes</span></h2>
    <div class="latest_post_container">
      <ul class="latest_postnav">
        <% @posts2.each do |post| %>
        <li>
          <div class="media"> <%= link_to image_tag(post.images[0].url), post, class: "media-left" %>
            <div class="media-body"> <%= link_to post.title, post, class: 'catg_title' %> </div>
          </div>
        </li>
          <% end %>
      </ul>
    </div>
  </div>
</div>
