<%= render 'front/devise_tools' %>


<header id="header">
  <div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12">
      <div class="header_top">
        <div class="header_top_left">
          <ul class="top_nav">
            <li> <%= link_to 'home', root_path %> </li>
            <li><a href="#">About</a></li>
            <li><a href="http://asoiu.com/">ASOIU</a></li>
            <li>
            <% if current_user %>
              <%= link_to "log out", destroy_user_session_path, method: 'delete' %>
            <% else %>
              <%= link_to "log in", new_user_session_path %>
            <% end %>
            </li>
          </ul>
        </div>
      </div>
    </div>
  </div>
</header>



<div class="col-lg-12 col-md-12 col-sm-12">
  <div class="header_bottom">
    <div class="logo_area"><a href="#" class="logo"><img src="/images/logo.jpg" alt=""></a></div>
  </div>
</div>
</div>
</header>
<section id="navArea">
  <nav class="navbar navbar-inverse" role="navigation">

    <div id="navbar" class="navbar-collapse collapse">
      <ul class="nav navbar-nav main_nav">
        <% @post_types2.each do |type| %>
          <li> <%= link_to type.title, posts_path(post_type_id: type.id) %> </li>
        <% end %>
        </ul>
        <ul class="nav navbar-nav navbar-right">
        <li> <%= link_to 'New Post', new_post_path %> </li>
      </ul>
    </div>
  </nav>

</section>